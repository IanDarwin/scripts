#!/bin/sh

# Generate missing test files.

for d in *.java
do
	testname=`echo $d | sed 's/\.java/Test&/'`
	if [ ! -f $D/${testname} ]; then
	echo "create $D/${testname}"

	## XXX $D/${testname}
	cat > ${testname} <<!EOF
package ca.sqlpower.matchmaker.hibernate;

import junit.framework.TestCase;

/**
 * Generated by /tmp/id
 */
public class `echo ${testname}|sed 's/.java//'` extends TestCase {

	public void testCopyOf() {
		fail("Not written yet");
	}
}
!EOF
	else
	echo ALREADY HERE $testname
	fi
done
